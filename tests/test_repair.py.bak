from llm_nature_indention_compensation.repair import compile_and_repair


    def test_compile_and_repair_fixes_simple_indent_error():
        code = """def f():
    if True:
      x = 1
    return x
"""
        fixed, err = compile_and_repair(code, max_iters=6)
        assert err is None
        compile(fixed, "<string>", "exec")
